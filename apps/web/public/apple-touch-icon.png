<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 180 180">
  <!-- Background with gradient -->
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2563eb;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="180" height="180" rx="40" fill="url(#grad)"/>
  
  <!-- Keyboard key shape -->
  <rect x="36" y="45" width="108" height="90" rx="14" fill="#ffffff" opacity="0.95"/>
  
  <!-- Letter T for TypeForge -->
  <text x="90" y="112" font-family="-apple-system, BlinkMacSystemFont, Arial, sans-serif" font-size="64" font-weight="bold" fill="#2563eb" text-anchor="middle">T</text>
  
  <!-- Subtle shadow/depth -->
  <rect x="36" y="131" width="108" height="7" rx="3" fill="#1e40af" opacity="0.3"/>
